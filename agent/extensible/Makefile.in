
# May need an ANSI compiler for extensible.c depending on whether or
# not SECURITYEXCEPTIONS is defined in config.h 

SHELL = /bin/sh
srcdir = @srcdir@
VPATH = @srcdir@
@SET_MAKE@

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I../.. -I$(srcdir)/../.. -I$(srcdir)/../../snmplib \
	-I$(srcdir)/.. -I.. -I$(srcdir)/../mibgroup -I../mibgroup
LDFLAGS=@LDFLAGS@

CC = @CC@ $(CPPFLAGS)

OBJS = misc.o read_config.o extensible.o hpmibs.o proc.o exec.o \
       versioninfo.o pass.o
SRCS = misc.c read_config.c extensible.c hpmibs.c proc.c exec.c \
       versioninfo.c pass.c
 

all: $(OBJS)

TAGS: $(SRCS)
	etags *.c *.h

clean:
	rm -f *.o

# Warning:  ugly make hack
tmp.make:
	if [ -d CVS ]; then \
	perl -e 'open(X,"cvs status -v versioninfo.c|"); $$ver = "";\
        while (($$_ = <X>) && (length($$ver) < 1)) { if (/Existing Tags/)\
                              {$$_ = <X>;  ($$x,$$ver) = split;};}\
	close(X); $$curver = ""; open(X,"versioninfo.c"); open(O,">tmp.make");\
        while (($$_ = <X>) && (length($$curver) < 5)) \
	{ s/VersionInfo="(.*)"/VersionInfo="$$ver"/; print O $$_;}\
	close(X);' ;\
	mv -f tmp.make versioninfo.c ;\
	fi

depend: 
	makedepend $(CPPFLAGS) *.c
	perl -n -i.bak -e ' if (/^# DO NOT DELETE THIS LINE/) {$$doit=1;} if ($$doit == 1) {s#/usr/include/[^\s]+##g ; print if (! /^\w+\.o:\s*$$/);} else {print;}' Makefile

