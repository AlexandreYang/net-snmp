#
# Makefile to install mib files.
#

SHELL = /bin/sh
srcdir = @srcdir@
VPATH = @srcdir@
@SET_MAKE@

libdir = @libdir@
snmplibdir = $(libdir)/snmp
mibdir = $(snmplibdir)/mibs

INSTALL = @INSTALL@

MIBS=UCD-SNMP-MIB.txt EtherLike-MIB.txt HOST-RESOURCES-MIB.txt \
	IANAifType-MIB.txt IF-MIB.txt IP-MIB.txt RFC1155-SMI.txt \
	RFC1213-MIB.txt RFC1271-MIB.txt SNMPv2-CONF.txt \
	SNMPv2-M2M-MIB.txt SNMPv2-MIB.txt SNMPv2-PARTY-MIB.txt \
	SNMPv2-SMI.txt SNMPv2-TC.txt SNMPv2-TM.txt TCP-MIB.txt \
	UDP-MIB.txt

all:

install: installdirs
	@for i in $(MIBS) ; do $(INSTALL) $(srcdir)/$$i $(mibdir) ; echo "install: installed $$i in $(mibdir)" ; done

installdirs:
	@$(SHELL) $(srcdir)/../mkinstalldirs $(mibdir)
