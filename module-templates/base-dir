#
# Set your net-snmp base source directory here
#
NETSNMP_SRC_DIR=~/snmp/main

if ! test -d $NETSNMP_SRC_DIR/agent/mibgroup ; then
	echo "could not find the 'agent/mibgroup' directory in '$NETSNMP_SRC_DIR'."
	exit 1
fi

#
# dont use default dir names (hidden .dirs), since we aren't in the src dir
#
UPDATE_ORIG=$PWD/orig
UPDATE_NEW=$PWD/new
UPDATE_MERGED=$PWD/merged
UPDATE_BACKUP=$PWD/backup
UPDATE_PATCH=$PWD/patch

