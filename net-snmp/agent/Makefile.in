#
# Makefile for snmpd
#

TARG		= snmpd
LIBTARG		= libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION)
USELIBS		= ../snmplib/libsnmp.$(SHLIB_EXTENSION)$(SHLIB_VERSION)
LOCAL_LIBS	= -L../snmplib -L.
LIBS		= $(CC_RUNTIME_ARG) -lagent -lsnmp @WRAPLIBS@ @LIBS@

CPPFLAGS	= -I.. -I$(srcdir)/.. -I$(srcdir)/../snmplib		\
		  -I$(srcdir) -I. -I$(srcdir)/mibgroup -Imibgroup @CPPFLAGS@

CC		= @CC@ $(CPPFLAGS)

MIBOBJS		= @mibgroup_list_o@

PERSISTENT_DIRECTORY=@PERSISTENT_DIRECTORY@

AGENTOBJS=snmpd.o kernel.o auto_nlist.o
LIBAGENTOBJS=snmp_agent.o snmp_vars.o agent_read_config.o agent_registry.o \
	agent_trap.o

INSTALLHEADERS=agent_read_config.h agent_registry.h agent_trap.h auto_nlist.h \
	ds_agent.h snmp_agent.h snmp_vars.h var_struct.h mibgroup/util_funcs.h \
	mibgroup/mibincl.h mibgroup/header_complex.h

all: subdirs snmpd libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION)


subdirs:
	cd mibgroup; $(MAKE); cd ..
#	cd dlmods; $(MAKE); cd ..

getkstat: getkstat.o
	$(CC) $(CFLAGS) -o $@ $? $(LOCAL_LIBS) $(LIBS)

getkstat.o: mibgroup/kernel_sunos5.c
	$(CC) $(CFLAGS) -o $@ -D_GETKSTAT_TEST -DDODEBUG -c $? 

getmibstat: getmibstat.o
	$(CC) $(CFLAGS) -o $@ $? $(LOCAL_LIBS) $(LIBS)

getmibstat.o: mibgroup/kernel_sunos5.c
	$(CC) $(CFLAGS) -o $@ -D_GETMIBSTAT_TEST -DDODEBUG -c $? 

snmpd:	${AGENTOBJS} $(USELIBS) libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION)
	${CC} -o $@ ${AGENTOBJS} $(LOCAL_LIBS) ${LDFLAGS} ${LIBS}  


libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION):    ${LIBAGENTOBJS} ${MIBOBJS} 
	$(SHLIB_LD_CMD) libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION) ${LIBAGENTOBJS} ${MIBOBJS} 
	$(RANLIB) libagent.$(SHLIB_EXTENSION)$(SHLIB_VERSION)

clean:
	rm -f *.o ${TARG} ${LIBTARG}
	cd dlmods; make clean; cd ..
	cd mibgroup; make clean


install:  installdirs
	@$(INSTALL) $(TARG) $(sbindir) ; echo "install:  installed $(TARG) in $(sbindir) "
	@for i in $(INSTALLHEADERS) ; do $(INSTALL) $(srcdir)/$$i $(includedir) ; echo "install:  installed $$i in $(includedir)" ; done
#	cd dlmods ; $(MAKE) install; cd ..

installdirs:
	@$(SHELL) $(srcdir)/../mkinstalldirs $(sbindir) $(snmplibdir) $(PERSISTENT_DIRECTORY)


dependlocal: 
	makedepend $(CPPFLAGS) $(srcdir)/*.c

depend: dependlocal
	cd mibgroup; $(MAKE) depend; cd ..

nosysdepend:
	cd mibgroup; $(MAKE) nosysdepend; cd ..
	makedepend $(CPPFLAGS) *.c
	perl -n -i.bak -e ' if (/^# DO NOT DELETE THIS LINE/) {$$doit=1;} if ($$doit == 1) {s#/usr/(include|lib)/[^\s]+##g ; print if (! /^\w+\.o:\s*$$/);} else {print;}' Makefile

makefileindepend: nosysdepend
	cd mibgroup; $(MAKE) makefileindepend; cd ..
	perl -e ' rename("Makefile.in","Makefile.in.bak"); open(F,"Makefile.in.bak"); open(G,"Makefile"); open(O,">Makefile.in"); $$_ = <F>; while(!/^# DO NOT DELETE THIS LINE/) { print O $$_; $$_ = <F>; } print O $$_; $$_ = <G>; while(!/^# DO NOT DELETE THIS LINE/) { $$_ = <G>; } while (<G>) {print O $$_;} '

