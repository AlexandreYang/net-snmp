#
# Makefile for snmpget, snmpwalk, snmpbulkwalk, snmptest, snmptranslate,
# snmptrapd, snmptable, snmpset, snmpgetnext
#
TARG=	snmpwalk snmpbulkwalk snmptest snmptranslate snmpget snmptrapd snmpset snmpgetnext
LIBS=	@LIBS@ ../lib/libsnmp.a

CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@ -I.. -I../snmplib
LDFLAGS=@LDFLAGS@

CC = @CC@ $(CPPFLAGS)

all: $(TARG)

snmpwalk:    snmpwalk.o
	${CC} -o $@ snmpwalk.o ${LDFLAGS} ${LIBS} 

snmpbulkwalk:    snmpbulkwalk.o
	${CC} -o $@ snmpbulkwalk.o ${LDFLAGS} ${LIBS} 

snmptranslate:    snmptranslate.o
	${CC} -o $@ snmptranslate.o ${LDFLAGS} ${LIBS} 

snmpget:    snmpget.o
	${CC} -o $@ snmpget.o ${LDFLAGS} ${LIBS} 

snmptest:    snmptest.o
	${CC} -o $@ snmptest.o ${LDFLAGS} ${LIBS} 

snmptrapd:    snmptrapd.o
	${CC} -o $@ snmptrapd.o ${LDFLAGS} ${LIBS} 

snmpset:    snmpset.o
	${CC} -o $@ snmpset.o ${LDFLAGS} ${LIBS} 

snmpgetnext:    snmpgetnext.o
	${CC} -o $@ snmpgetnext.o ${LDFLAGS} ${LIBS} 

clean:
	rm -f *.o ${TARG}

install:
	cp ${TARG} ../bin

depend: 
	makedepend $(CPPFLAGS) *.c
	perl -n -i.bak -e ' if (/^# DO NOT DELETE THIS LINE/) {$$doit=1;} if ($$doit == 1) {s#/usr/include/[^\s]+##g ; print if (! /^\w+\.o:\s*$$/);} else {print;}' Makefile

