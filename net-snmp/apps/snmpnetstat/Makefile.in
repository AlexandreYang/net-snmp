#
# snmpnetstat Makefile
#
#

top_builddir=../..
SRCS=	inet.c inet6.c if.c main.c route.c winstub.c ffs.c iquery.c strlcpy.c
OBJS=	inet.o inet6.o if.o main.o route.o winstub.o ffs.o iquery.o strlcpy.o
LOBJS=	inet.lo inet6.lo if.lo main.lo route.lo winstub.lo ffs.lo iquery.lo strlcpy.lo
TARG= snmpnetstat53$(EXEEXT)
INSTALLBINPROGS=$(TARG)
INSTALLBINSCRIPTS=$(srcdir)/snmpnetstat$(EXEEXT)

CPPFLAGS=-I../.. -I$(srcdir)/../.. -I$(srcdir)/../../snmplib @CPPFLAGS@

#LOCAL_LIBS = -L../../snmplib
LOCAL_LIBS =
#LIBS=	 $(CC_RUNTIME_ARG) -lsnmp @LIBS@
USELIBS= ../../snmplib/libsnmp.$(LIB_EXTENSION)$(LIB_VERSION)
LIBS=	 ../../snmplib/libsnmp.$(LIB_EXTENSION)$(LIB_VERSION) @LIBS@

all: $(TARG)

snmpnetstat53$(EXEEXT): ${LOBJS} ${USELIBS}
	${LINK} -o $@ ${LOBJS} ${LOCAL_LIBS} ${LDFLAGS} ${LIBS}

distclean: clean

clean:
	rm -f core *.o *.lo *.exe $(TARG) $(SBINTARG)
	rm -rf .libs

cleandir: clean
	rm -f ${MAN} tags .depend

lint: ${SRCS}
	lint ${CFLAGS} ${SRCS}

tags: ${SRCS}
	ctags ${SRCS}

installdirs:
	@$(SHELL) $(srcdir)/../../mkinstalldirs $(snmplibdir) $(bindir)

install: installdirs $(TARG)
	@for i in $(TARG) ; do $(INSTALL) $$i $(bindir) ; echo "install:  installed $$i in $(bindir)" ; done

depend: 
	makedepend $(CPPFLAGS) -o.lo $(srcdir)/*.c

nosysdepend:
	makedepend $(CPPFLAGS) -o.lo *.c
	perl -n -i.bak $(top_srcdir)/makenosysdepend.pl Makefile

makefileindepend: nosysdepend
	perl $(top_srcdir)/makefileindepend.pl

