#
# Makefile for snmpget, snmpwalk, snmpbulkwalk, snmptest, snmptranslate,
# snmptrapd, snmptable, snmpset, snmpgetnext, and other utilities.
#

top_builddir 	= ..

TARG		= snmpget snmpgetnext snmpset snmpusm			\
			snmpwalk snmpbulkwalk snmptable			\
			snmptrap snmpbulkget				\
			snmptranslate snmpstatus snmpdelta snmptest	\
			encode_keychange snmpdf

SBINTARG	= snmptrapd
SUBDIRS		= snmpnetstat

USELIBS		= ../snmplib/libsnmp.$(LIB_EXTENSION)$(LIB_VERSION)
LOCAL_LIBS	= -L../snmplib 
#LIBS		= $(CC_RUNTIME_ARG) -lsnmp @LIBS@
LIBS		= $(USELIBS) @LIBS@

PARSEOBJS	= snmp_parse_args.o

CPPFLAGS	= -I.. -I$(srcdir)/.. -I$(srcdir)/../snmplib @CPPFLAGS@
OSUFFIX		= lo


all: $(TARG) $(SBINTARG) subdirs


subdirs:
	for i in $(SUBDIRS) ; do \
        ( cd $$i ; $(MAKE) ) ; \
	done

snmpwalk:    snmpwalk.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpwalk.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpbulkwalk:    snmpbulkwalk.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpbulkwalk.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpbulkget:    snmpbulkget.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpbulkget.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmptranslate:    snmptranslate.$(OSUFFIX) $(USELIBS)
	$(LINK) -o $@ snmptranslate.$(OSUFFIX) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpstatus:    snmpstatus.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpstatus.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpget:    snmpget.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpget.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpdelta:    snmpdelta.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpdelta.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmptable:    snmptable.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmptable.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmptest:    snmptest.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmptest.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmptrapd:    snmptrapd.$(OSUFFIX) snmptrapd_handlers.$(OSUFFIX) $(USELIBS)
	$(LINK) -o $@ snmptrapd.$(OSUFFIX) snmptrapd_handlers.$(OSUFFIX) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmptrap:    snmptrap.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmptrap.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 
	rm -f snmpinform
	$(LN_S) snmptrap snmpinform

snmpset:    snmpset.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpset.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpusm:    snmpusm.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpusm.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpgetnext:    snmpgetnext.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpgetnext.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

encode_keychange:    encode_keychange.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ encode_keychange.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 

snmpdf:    snmpdf.$(OSUFFIX) $(PARSEOBJS) $(USELIBS)
	$(LINK) -o $@ snmpdf.$(OSUFFIX) $(PARSEOBJS) $(LOCAL_LIBS) ${LDFLAGS} ${LIBS} 


clean:
	rm -f *.o *.lo $(TARG) $(SBINTARG)
	rm -rf .libs
	for i in $(SUBDIRS) ; do \
	( cd $$i ; $(MAKE) clean ) ; \
	done


install: installdirs subdirs $(TARG)
	for i in $(SUBDIRS) ; do \
        ( cd $$i ; $(MAKE) install ) ; \
	done
	@for i in $(TARG) ; do $(INSTALL) $$i $(bindir) ; echo "install:  installed $$i in $(bindir)" ; done
	rm -f $(bindir)/snmpinform
	(cd $(bindir) && $(LN_S) snmptrap snmpinform)
	@for i in $(SBINTARG) ; do $(INSTALL) $$i $(sbindir) ; echo "install:  installed $$i in $(sbindir)" ; done

installdirs:
	@$(SHELL) $(srcdir)/../mkinstalldirs $(snmplibdir) $(bindir)


dependlocal: 
	makedepend $(CPPFLAGS) $(srcdir)/*.c

depend: dependlocal
	for i in $(SUBDIRS) ; do \
        ( cd $$i ; $(MAKE) depend ) ; \
	done

nosysdepend: 
	for i in $(SUBDIRS) ; do \
        ( cd $$i ; $(MAKE) nosysdepend ) ; \
	done
	makedepend $(CPPFLAGS) *.c
	perl -n -i.bak $(top_srcdir)/makenosysdepend.pl Makefile

makefileindepend: nosysdepend
	cd snmpnetstat; $(MAKE) makefileindepend; cd ..
	perl $(top_srcdir)/makefileindepend.pl

